{
  "schemaVersion": 1,
  "id": "legacybetterconfig",
  "version": "${version}",
  "name": "LegacyBetterConfig",
  "description": "A very powerful and easy to use command based configuration library for servers and clients.",
  "authors": [
    "Crosby"
  ],
  "contributors": [
    "xpple"
  ],
  "contact": {
    "sources": "https://github.com/RacoonDog/LegacyBetterConfig/"
  },
  "license": "LGPL-3.0-or-later",
  "icon": "assets/betterconfig/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "dev.xpple.betterconfig.BetterConfigClient"
    ],
    "main": [
      "dev.xpple.betterconfig.BetterConfig"
    ]
  },
  "mixins": [
    "legacybetterconfig.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.17",
    "minecraft": "1.8.9",
    "java": ">=8",
    "legacy-fabric-api": "*"
  },
  "custom": {
    "modmenu": {
      "badges": [
        "library"
      ]
    }
  }
}
